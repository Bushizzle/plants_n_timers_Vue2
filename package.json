{
  "name": "flimers-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=hmr_development vue-cli-service serve",
    "build": "vue-cli-service build --no-clean --mode development",
    "dev": "vue-cli-service build --no-clean",
    "dev:watch": "vue-cli-service build --no-clean --watch",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "jsondb:ai-search": "json-server -d 100 -p 3030 --watch src/apps/ai-search/db/ai-search.db.json --routes src/apps/ai-search/db/ai-search.routes.json",
    "jsondb:synonyms": "json-server -d 100 -p 3030 --watch src/apps/synonyms/db/synonyms.db.json --routes src/apps/synonyms/db/synonyms.routes.json"
  },
  "dependencies": {
    "@vue/cli": "^4.5.11",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.3",
    "css-vars-ponyfill": "^2.4.2",
    "json-server": "^0.16.1",
    "mongoose": "^5.12.1",
    "ts-debounce": "^2.0.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-click-outside": "martiiian/click-outside#master",
    "vue-i18n": "^8.24.1",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.5",
    "vue-toasted": "^1.1.28",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@mdi/font": "^5.0.45",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-typescript": "~4.2.0",
    "@vue/cli-plugin-unit-jest": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "less-loader": "^5.0.0",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "typescript": "~3.7.5",
    "vue-template-compiler": "^2.6.11",
    "vuex-module-decorators": "^0.16.1"
  }
}
